{
  "name": "quickshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "prisma generate",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:production": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/deploy-migrations.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:status": "prisma migrate status",
    "worker": "ts-node workers/automation-worker.ts",
    "queue:stats": "curl http://localhost:3000/api/automations/queue",
    "queue:clean": "curl -X DELETE 'http://localhost:3000/api/automations/queue?clean=true'",
    "queue:test": "ts-node scripts/test-automation-queue.ts",
    "redis:start": "redis-server --daemonize yes",
    "redis:stop": "redis-cli shutdown",
    "redis:check": "redis-cli ping",
    "remove-gift-items": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/remove-gift-items-from-cart.ts",
    "seed:bundles": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/seed-bundles-example.ts",
    "download:image": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/download-product-image.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
